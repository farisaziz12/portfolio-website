{
  "name": "faziz-dev",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm run --parallel -r dev",
    "build": "pnpm run -r build",
    "lint": "pnpm run -r lint",
    "typecheck": "pnpm run -r typecheck",
    "studio": "pnpm --filter studio dev",
    "web": "pnpm --filter web dev",
    "migrate": "node --env-file=.env --import tsx scripts/migrate-content.ts",
    "remove-duplicates": "tsx --env-file=.env scripts/remove-duplicates.ts"
  },
  "dependencies": {
    "@sanity/client": "^6.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.7.2",
    "eslint": "^9.16.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.14.4"
}
